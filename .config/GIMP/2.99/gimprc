# GIMP gimprc
# 
# This is your personal gimprc file.  Any variable defined in this file takes
# precedence over the value defined in the system-wide gimprc:
# /etc/gimp/2.99/gimprc
# Most values can be set within GIMP by changing some options in the
# Preferences dialog.

(config-version "2.99.10")
(import-raw-plug-in "${gimp_plug_in_dir}/plug-ins/file-raw-placeholder/file-raw-placeholder")
(monitor-xresolution 81)
(monitor-yresolution 81)
(toolbox-groups no)
(image-convert-indexed-max-colors 4)
(image-convert-indexed-dither-type fs)
(layer-new-name "bingo")
(layer-add-mask-type selection)
(layer-merge-type clip-to-image)
(selection-feather-radius 2)
(selection-grow-radius 3)
(fill-options
    (style solid)
    (antialias yes)
    (feather no)
    (feather-radius 10))
(stroke-options
    (style solid)
    (antialias yes)
    (feather no)
    (feather-radius 10)
    (method paint-method)
    (width 3)
    (unit pixels)
    (cap-style butt)
    (join-style miter)
    (miter-limit 10)
    (dash-offset 0)
    (dash-info 0)
    (emulate-brush-dynamics no))


# end of gimprc
